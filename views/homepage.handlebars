<body class="blogposts">
{{#each blogPosts}}
<div class="row mb-4 project">
 <div class="col-md-5">
  <h2>
   <a href="/blogPost/{{id}}">{{title}}</a>
  </h2>
  <p>Created by {{user.username}} on {{format_date date_created}}</p>
 </div>
 <div class="col-md-7">
  <p>
   {{post}}
  </p>
 </div>
</div>
{{/each}}
</body>